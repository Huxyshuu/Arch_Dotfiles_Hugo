	"custom/player": {
		"exec": "~/.config/waybar/scripts/player_block.py",
		"return-type": "json",
		"on-click": "playerctl play-pause",
		"on-scroll-up": "wpctl set-volume @DEFAULT_SINK@ 5%+",
		"on-scroll-down": "wpctl set-volume @DEFAULT_SINK@ 5%-",
		"max-length": 40,
		"interval": 1
