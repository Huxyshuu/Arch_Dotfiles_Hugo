	/*-----------Settings, Terminal, Colorpicker, Temperature, Idle-inhibitor------*/
	"custom/settings": {
		"format": "",
		"tooltip": false,
		"on-click": "systemsettings"
	},
	"custom/terminal": {
		"format": "",
		"tooltip": false,
		"on-click": "kitty"
	},
	"custom/chrome": {
		"format": "",
		"tooltip": false,
		"on-click": "exec google-chrome-stable"
	},
	// "temperature": {
	// 	"critical-threshold": 80,
	// 	"format": ""
	// },
	"custom/colorpicker": {
		"format": "{}",
		"return-type": "json",
		"interval": "once",
		"exec": "~/.config/waybar/scripts/Colorpicker.sh -j",
		"on-click": "~/.config/waybar/scripts/Colorpicker.sh",
		"signal": 1
	},
	"custom/clipboard": {
		"format": "",
		"tooltip": true,
		"tooltip-format": "Left Click: Open clipboard\nRight Click: Wipe clipboard",
		"on-click-right": "exec cliphist wipe",
		"on-click": "exec cliphist list | rofi -dmenu | xargs cliphist decode | wl-copy"
	},

